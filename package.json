    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "concurrently": "^7.6.0",
    "fs-extra": "^11.1.0",
    "localtunnel": "^2.0.2",
    "open": "^8.4.0",
    "open-cli": "^7.1.0",
    "rollup": "^3.7.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
